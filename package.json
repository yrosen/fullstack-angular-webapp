{
  "name": "fullstack-ng",
  "version": "0.0.0",
  "scripts": {
    "start": "rm -rf dist && concurrently \"npm run start:client\" \"npm run start:server\"",
    "start:server": "tsc-watch --project ./src/server --outDir ./dist/server --onSuccess \"nodemon ./dist/server/index.js\"",
    "start:client": "ng serve",
    "build:client": "ng build",
    "build:server": "tsc --project ./src/server --outDir ./dist/server",
    "test:client": "ng test",
    "test:server": "jest --config=./src/server/jest.config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "express": "^4.18.2",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.9",
    "@angular/cli": "~14.2.9",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jasmine": "~4.0.0",
    "@types/supertest": "^2.0.12",
    "concurrently": "^7.5.0",
    "jasmine-core": "~4.3.0",
    "jest": "^29.3.1",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^5.0.3",
    "typescript": "~4.7.2"
  }
}
